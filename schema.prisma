datasource postgresql {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model User {
  id       String @id @default(cuid())
  name     String @default("")
  email    String @unique @default("")
  password String
}

model Type {
  id     String  @id @default(cuid())
  name   String  @default("")
  assets Asset[] @relation("Asset_type")
}

model Classification {
  id     String  @id @default(cuid())
  name   String  @default("")
  assets Asset[] @relation("Asset_classification")
}

model Asset {
  id               String          @id @default(cuid())
  ticket           String          @default("")
  type             Type?           @relation("Asset_type", fields: [typeId], references: [id])
  typeId           String?         @map("type")
  classification   Classification? @relation("Asset_classification", fields: [classificationId], references: [id])
  classificationId String?         @map("classification")
  active           Boolean         @default(true)

  @@index([typeId])
  @@index([classificationId])
}